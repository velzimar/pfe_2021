{{ form_start(form) }}
    <div class="row">
        <div class="col md-4">{{ form_row(form.email) }}</div>
    </div>
    <div class="row">
        <div class="col md-2">{{ form_row(form.password) }}</div>
    </div>
    {{ form_rest(form) }}
    <button class="btn">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
